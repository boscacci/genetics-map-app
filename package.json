{
  "name": "interactive-map-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://boscacci.github.io/genetics-map-app",
  "main": "index.js",
  "scripts": {
    "prestart": "node scripts/hash-secret.js && node scripts/update-app-hash.js",
    "start": "dotenv -e .env.generated -- react-scripts start",
    "prebuild": "node scripts/hash-secret.js && node scripts/update-app-hash.js",
    "build": "dotenv -e .env.generated -- react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sync-secrets": "node scripts/sync-secrets.js",
    "setup:hooks": "bash scripts/setup-git-hooks.sh",
    "encrypt-data": "node scripts/process-data.js",
    "prepare-local": "npm run encrypt-data && node scripts/hash-secret.js && node scripts/update-app-hash.js",
    "verify": "node scripts/verify-sync.js",
    "full-sync": "npm run prepare-local && npm run verify && npm run sync-secrets",
    "test:gcp": "node scripts/test-gcp-apis.js",
    "setup:sheet": "node scripts/configure-existing-sheet.js",
    "promote": "node scripts/promote-to-production.js",
    "export:production": "node scripts/export-production-to-csv.js",
    "backup:production": "node scripts/backup-production.js",
    "clean:data": "python scripts/clean_and_validate.py"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/leaflet": "^1.9.20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "crypto-js": "^4.2.0",
    "googleapis": "^171.4.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-leaflet-markercluster": "^5.0.0-rc.0",
    "react-router-dom": "^7.7.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.10.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "dotenv-cli": "^7.0.0",
    "ts-node": "^10.9.2"
  }
}
